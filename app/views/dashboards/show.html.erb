<h1>Dashboard</h1>
Welcome <%= current_user.email %> - <%= link_to "Sign out", sign_out_path, method: :delete %>

<%= form_for(@text_shout) do |form| %>
  <%= form.text_field :body, placeholder: "Shout here" %>
  <%= form.submit "Shout!" %>
<% end %>
<%= form_for(@photo_shout) do |form| %>
  <%= form.file_field :photo %>
  <%= form.submit "Shout!" %>
<% end %>

<div id="shouts">
  <%= render @shouts %>
</div>
